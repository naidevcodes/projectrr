<%= form_for(@assignment) do |f|%>
<%= render 'shared/errors', object: @assignment %>
  
<fieldset>
  <div class="field">
    <%= f.label :project %><br />
    <%= f.select :project_id, Project.all.collect { |p| [ p.name, p.id ] } %>
  </div>
  <div class="field">
    <%= f.label :resource %><br />
    <%= f.select :resource_id, Resource.all.collect { |p| [ p.name, p.id ] } %>
  </div>
  <div class="field">
    <%= f.label :start_date %><br />
    <%= f.text_field :start_date %>
  </div>
  <div class="field">
    <%= f.label :end_date %><br />
    <%= f.text_field :end_date %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
</fieldset>
<% end %>